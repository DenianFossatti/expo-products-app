{
  "name": "expo-products-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "yarn prebuild && expo run:ios",
    "web": "yarn prebuild && expo start --web",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "prebuild": "expo prebuild",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "typecheck": "tsc --noEmit",
    "env:check": "tsx --env-file .env ./src/env.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "@tanstack/react-query": "^5.85.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^53.0.20",
    "expo-constants": "~17.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.3",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.542.0",
    "nativewind": "^4.0.1",
    "onesignal-expo-plugin": "^2.0.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-onesignal": "^5.2.13",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.20.0",
    "react-native-worklets": "^0.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3"
